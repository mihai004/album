<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

        <services>
            <service id="blaga_date_format.date_format" class="Blaga\DateFormatBundle\DateFormat" public="true">
                <argument type="service" id="blaga_date_format.date_format_provider"/>
            </service>

            <service id="blaga_date_format.date_format_provider" class="Blaga\DateFormatBundle\DateFormatProvider" />

            <service id="Blaga\DateFormatBundle\DateFormat" alias="blaga_date_format.date_format" public="false" />
        </services>
</container>